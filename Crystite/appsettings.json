{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Debug" ],
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    },
    "Headless": {
        "resonitePath": "/var/lib/crystite/Resonite",
        "assetCleanupInterval": "06:00:00",
        "maxAssetAge": "07:00:00:00"
    },
    "Resonite": {
        "$schema": "https://raw.githubusercontent.com/Neos-Metaverse/JSONSchemas/main/schemas/NeosHeadlessConfig.schema.json",
        "dataFolder": "/var/lib/crystite/data",
        "cacheFolder": "/var/lib/crystite/data",
        "startWorlds": [
            {
                "sessionName": "Crystite - .NET 7 Headless",
                "hideFromPublicListing": false,
                "isEnabled": true,
                "maxUsers": 32,
                "accessLevel": "Anyone",
                "defaultFriendRole": "Builder",
                "defaultAnonymousRole": "Builder",
                "defaultVisitorRole": "Builder",
                "loadWorldPresetName": "Grid"
            }
        ]
    }
}
